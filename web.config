<configuration>
 <system.webServer>
  <httpProtocol>
    <customHeaders>
      <add name="Cache-Control" value="no-cache" />
      <add name="Pragma" value="no-cache" />
      <add name="Expires" value="-1" />
    </customHeaders>
  </httpProtocol>
  <rewrite>
    <rules>
      <rule name="Redirect to non-www" stopProcessing="true">
        <match url="(.*)" negate="false"></match>
        <action type="Redirect" url="https://celebri.com.br/dashboard/{R:1}"></action>
        <conditions logicalGrouping="MatchAny">>
          <add input="{HTTP_HOST}" pattern="^celebri\.com.br$" negate="true"></add>
          <add input="{HTTPS}" pattern="off" ignoreCase="true" />
        </conditions>
      </rule>
    </rules>
  </rewrite>
 </system.webServer>
</configuration>

